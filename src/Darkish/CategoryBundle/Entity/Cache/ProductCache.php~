<?php

namespace Darkish\CategoryBundle\Entity\Cache;

use Doctrine\ORM\Mapping as ORM;

/**
 * ProductCache
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class ProductCache
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="productId", type="integer")
     * @ORM\OneToOne(targetEntity="Darkish\CategoryBundle\Entity\Product")
     */
    private $productId;

    /**
     * @var array
     *
     * @ORM\Column(name="json", type="json_array")
     */
    private $json;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    

    /**
     * Set json
     *
     * @param array $json
     *
     * @return ProductCache
     */
    public function setJson($json)
    {
        $this->json = $json;

        return $this;
    }

    /**
     * Get json
     *
     * @return array
     */
    public function getJson()
    {
        return $this->json;
    }
}

