    <link href="{{ asset('bundles/darkishwebsite/plugins/fotorama/fotorama.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/darkishwebsite/bower_components/jquery-modal/jquery.modal.css') }}" rel="stylesheet" />
